{
  "name": "benjamminf/craft-nocache",
  "description": "A Twig extension to escape caching inside cache blocks",
  "version": "2.0.0",
  "type": "craft-plugin",
  "keywords": [
    "cms",
    "craftcms",
    "plugin"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Fleming",
      "homepage": "https://github.com/benjamminf"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0"
  },
  "support": {
    "issues": "https://github.com/benjamminf/craft-nocache/issues",
    "source": "https://github.com/benjamminf/craft-nocache",
    "rss": "https://github.com/benjamminf/craft-nocache/commits/master.atom"
  },
  "autoload": {
    "psr-4": {
      "benf\\nocache\\": "src/"
    }
  },
  "extra": {
    "handle": "nocache",
    "name": "No-Cache",
    "class": "benf\\nocache\\Plugin",
    "changelogUrl": "https://github.com/benjamminf/craft-nocache/blob/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/benjamminf/craft-nocache/archive/master.zip"
  }
}
